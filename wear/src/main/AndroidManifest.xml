            android:name=".common.receiver.AAPSReceiver"
            android:enabled="true"
            android:exported="true"
            tools:ignore="ExportedReceiver">
            <intent-filter>
                <action android:name="info.nightscout.androidaps.status" />
            </intent-filter>
        </receiver>
