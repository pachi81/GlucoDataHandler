        <receiver android:name=".common.receiver.GlucoseDataReceiver" android:enabled="true" android:exported="true"
            tools:ignore="ExportedReceiver">
            <intent-filter>
                <action android:name="glucodata.Minute"/>
            </intent-filter>
        </receiver>
